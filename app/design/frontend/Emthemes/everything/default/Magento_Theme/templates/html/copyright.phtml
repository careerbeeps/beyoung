<script>
require([
            'jquery'
        ], function(jQuery){
            (function(jQuery) {
				jQuery(".page-header.em-header-01").addClass("sticky-head");
              var lastScrollTop = 0;
              jQuery(window).scroll(function(event){
               var st =jQuery(this).scrollTop();
               if (st > lastScrollTop && st>160){
                 jQuery(".page-header.em-header-01").removeClass("sticky-head");
               } else {
                  // upscroll code
                  jQuery(".page-header.em-header-01").addClass("sticky-head");
               }
               lastScrollTop = st;
            });

             

            })(jQuery);
        });

</script>


